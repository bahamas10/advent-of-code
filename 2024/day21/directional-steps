#!/usr/bin/env bash
declare -A directional_steps=(["A|v"]="<v"
["<|v"]=">"
["^|v"]="v"
["<|^"]=">^"
["^|^"]=""
["A|^"]="<"
["A|A"]=""
["^|A"]=">"
["<|A"]=">>^"
["<|>"]=">>"
["^|<"]="v<"
["<|<"]=""
["^|>"]="v>"
["A|>"]="v"
["A|<"]="v<<"
["v|<"]="<"
["v|>"]=">"
["v|^"]="^"
["v|A"]="^>"
["v|v"]=""
[">|<"]="<<"
[">|>"]=""
[">|^"]="<^"
[">|A"]="^"
[">|v"]="<"
)
